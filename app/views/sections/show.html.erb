<% @page_title = "Show Section" %>

<%= link_to('<< Back to List',{action:'index'},class:'btn btn-sm btn-dark') %>
<h2>Show Section</h2>

<div class="row">

	<table class="table" summary="Subjects detail view">
		<tr>
			<th>Name:</th>
			<td><%= @section.name %></td>
		</tr>
		<tr>
			<th>Position:</th>
			<td><%= @section.position %></td>
		</tr>
		<tr>
			<th>Visible?</th>
			<td><%= @section.visible ? 'Yes' : 'No' %></td>
		</tr>
		<tr>
			<th>Created:</th>
			<td><%= @section.created_at %></td>
		</tr>
		<tr>
			<th>Updated:</th>
			<td><%= @section.updated_at %></td>
		</tr>
	</table>


</div>